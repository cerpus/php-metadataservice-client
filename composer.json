{
  "name": "cerpus/metadataservice-client",
  "description": "Client to handle communication with the Cerpus MetadataService",
  "require": {
    "illuminate/support": "^6.0",
    "nesbot/carbon": "^2.0",
    "ramsey/uuid": "^3.8",
    "cerpus/cerpushelper": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "fzaninotto/faker": "^1.7",
    "mockery/mockery": "^1.0",
    "shrikeh/teapot": "^2.3"
  },
  "authors": [
    {
      "name": "Thomas Horn Sivertsen",
      "email": "thomass@cerpus.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cerpus\\MetadataServiceClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cerpus\\MetadataServiceClientTests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.cerpus.net/"
    },
    {
      "type": "composer",
      "url": "https://composer-3rdparty.cerpus.net/"
    }
  ]

}
